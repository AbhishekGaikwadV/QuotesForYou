{
  "name": "quotesforyou-client",
  "version": "2.0.0",
  "description": "Frontend for quotes serving website",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build"
  },
  "author": "Abhishek Gaikwad",
  "license": "ISC",
  "dependencies": {
    "@blueprintjs/core": "^5.10.2",
