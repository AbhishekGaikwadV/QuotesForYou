{
    "version": 2,
    "builds": [
      {
        "src": "api/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/build/**/*",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/build"
        }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "api/app.js" },
      { "src": "/(.*)", "dest": "/client/build/$1" }
    ]
  }
  